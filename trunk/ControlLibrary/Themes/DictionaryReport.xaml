<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="ReportHeader" TargetType="{x:Type Grid}">
        
    </Style>
    <Style x:Key="ReportHeaderLine" TargetType="{x:Type Label}">
        <Setter Property="VerticalContentAlignment" Value="Center"/>
    </Style>
    <Style x:Key="ReportDataItems" TargetType="{x:Type Grid}">
        <Setter Property="TextBlock.VerticalAlignment" Value="Center"/>
    </Style>
    <Style x:Key="ReportDataItemsLine" TargetType="{x:Type Label}">
        <Setter Property="VerticalContentAlignment" Value="Center"/>
    </Style>
</ResourceDictionary>