<UserControl xmlns:my="clr-namespace:ControlLibrary;assembly=ControlLibrary"  x:Class="UserControlLibrary.UCMenuMayIn"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d"
             d:DesignHeight="768" d:DesignWidth="924" Loaded="UserControl_Loaded" xmlns:my1="clr-namespace:UserControlLibrary">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="10*" />
            <ColumnDefinition Width="10*" />
        </Grid.ColumnDefinitions>

        <my1:UCMenuSetMayIn HorizontalAlignment="Stretch" x:Name="uCMenuSetMayIn" VerticalAlignment="Stretch" />
        <my1:UCMenu Grid.Column="1" HorizontalAlignment="Stretch" x:Name="UCMenu"  VerticalAlignment="Stretch" _IsBanHang="False" />
    </Grid>
</UserControl>